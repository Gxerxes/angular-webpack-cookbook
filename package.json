{
  "name": "angular-webpack-cookbook",
  "version": "0.0.0",
  "description": "Angular, Webpack cookbook generator",
  "main": "./index.js",
  "scripts": {
    "sync-wiki": "./scripts/sync-wiki.sh",
    "generate-gitbook": "gitbook build ./content ./gh-pages",
    "generate-wiki": "node ./scripts/generate-wiki.js",
    "deploy-gitbook": "node ./scripts/deploy-gh-pages.js",
    "deploy-wiki": "./scripts/deploy-wiki.sh",
    "generate-and-deploy": "npm run generate-gitbook && npm run generate-wiki && npm run deploy-gitbook && npm run deploy-wiki",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmachat/angular-webpack-cookbook.git"
  },
  "keywords": [
    "angular",
    "webpack"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmachat/angular-webpack-cookbook/issues"
  },
  "homepage": "https://github.com/dmachat/angular-webpack-cookbook",
  "dependencies": {
    "async": "^0.9.0",
    "fs-extra": "^0.18.2",
    "gh-pages": "^0.2.0",
    "gitbook-cli": "^0.3.3"
  }
}
